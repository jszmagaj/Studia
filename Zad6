//Zrealizuj procedurę, która liczy i zwraca wartość iloczynu skalarnego wektorów w przestrzeni R(n).


public class Main {


    public static void main(String[] args) {

    }

    static int wektor(int[] a, int[]b){

        int sum = 0;

        for(int i=0; i<a.length;i++){
            sum+=(a[i]*b[i]);
        }
        return sum;
    }
}
